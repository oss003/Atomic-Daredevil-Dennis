;---------------------------------------------------------------
; Player
;---------------------------------------------------------------

EVENT PLAYER

LET Q X				; Save player position
LET R Y

GETBLOCKS			; Check collectable blocks

IF H = 0			; Check if fire falling

  TABLEFALL

  IF CUSTOM			; Balloon trigger
    IF C 0			; Set balloon flag
      LET I Y
      ADD 16 TO I		; Check for customblock
      GETBLOCK X,I
      IF BLOCK CUSTOMBLOCK
        LET C 1
        SUBTRACT 17 FROM Y
        SPAWN 8,10		; Plot balloon
        ADD 17 TO Y
      ENDIF
    ENDIF
  ENDIF

  IF C 1			; If balloon on screen
    LET I 0			; Check if on platform
    IF Y 80			; Plaform 2
      LET I 1
    ENDIF
    IF Y 120			; Plaform 3
      LET I 1
    ENDIF
    IF Y 160			; Plaform 4
      LET I 1
    ENDIF

    IF I 0			; Not on platform
      ADD 1 TO Y		; Fall
    ELSE
      LET T 9			; On platform, reset speed
      LET C 0			; Reset balloon flag
    ENDIF

    EXIT			; Exit sub
  ENDIF

  LET S = T			; Set speed
  DIVIDE S BY 10

  IF S > 0			; Move player with speed delay

    REPEAT S

      IF F 1			; Check if snow
        IF AIRBORNE 0		; Check for jump
          BEEP 3		; Motor sound
        ENDIF
      ELSE
        BEEP 3			; Motor sound
      ENDIF

      IF X < 240		; Check leaving screen right
        SPRITERIGHT
      ELSE
        LET X 0
	IF Y < 128		; End of screen not reached
	  ADD 40 TO Y
 	  SCORE B
          AT 1,8
          PAPER 2
          INK 1
          SHOWSCORE 5
	ELSE 
 	  SCORE B		; End of screen reached
          AT 1,8
          PAPER 2
          INK 1
          SHOWSCORE 5
	  NEXTLEVEL
	  ADD 10 TO B
	ENDIF
      ENDIF
    ENDREPEAT
  ENDIF

  IF KEY RIGHT			; Accelerate
   IF T < 40
    ADD 1 TO T
   ENDIF
  ENDIF

  IF KEY LEFT			; Break, once per screen
   IF Z = 0
    STOPFALL
    LET T 9
    LET Z 1
   ENDIF
  ENDIF

  IF KEY FIRE			; Jump
   IF H = 0
     IF AIRBORNE 0		; If not in the air
       BEEP 50			; Jump sound
     ENDIF
     TABLEJUMP
   ENDIF
  ENDIF

ENDIF

IF KEY OPTION1
  LET LIVES 1
  KILL
ENDIF

IF DEADLY			; Check if hit static object
  IF H = 0
    STOPFALL
    LET FRAME 3			; If so, change player in fire
    SPAWN 2 3			; Plot player dead
    LET H 1
    LET S 0
    LET C 0
  ENDIF
ENDIF

;---------------------------------------------------------------
; Horizontal moving enemy, ambulance, sailing boat, snowball
;---------------------------------------------------------------

EVENT SPRITETYPE1

  IF COLLISION 0		; If hit player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  IF C 0			; Stop if balloon
    SPRITERIGHT			; Move horizontal enemy
    SPRITERIGHT
    ANIMATE
  ENDIF

;---------------------------------------------------------------
; Dead player
;---------------------------------------------------------------

EVENT SPRITETYPE2

  TABLEFALL

  IF AIRBORNE > 0		; If in air
    USER 0			; Noise
    SPRITERIGHT			; Move right
    SPRITERIGHT
  ELSE
    ADD 1 TO SETTINGA
    IF SETTINGA < 20		; Animate 20 times
      ANIMATE
    ELSE
      KILL
    ENDIF
  ENDIF

;---------------------------------------------------------------
; Spider
;---------------------------------------------------------------

EVENT SPRITETYPE3

  ADD 1 TO SETTINGA

  IF COLLISION 0		; If hit player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  IF SETTINGA 4			; Check move up/down
    IF D 0
      SPRITEDOWN		; Move down
      IF Y > 150		
        LET D 1			; If at bottom, change direction to up
      ENDIF
    ELSE
      SPRITEUP			; Move up
      IF Y < 142
        LET D 0			; If at top, change direction down
      ENDIF
    ENDIF
    ANIMATE
    LET SETTINGA 0
  ENDIF

;---------------------------------------------------------------
; Spider string
;---------------------------------------------------------------

EVENT SPRITETYPE4

  ADD 1 TO SETTINGA

  IF COLLISION 0		; If hit player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  IF SETTINGA 4			; Check move up/down
    IF D 0
      ANIMATE			; If down enlarge string
    ELSE
      ANIMBACK			; If up shorten string
    ENDIF
    LET SETTINGA 0
  ENDIF

;---------------------------------------------------------------
; Policeman/Red ball/ Skull
;---------------------------------------------------------------

EVENT SPRITETYPE5

  IF C 0			; Stop if balloon
    IF SETTINGA > 0		; Delaytime start jump
      SUBTRACT 1 FROM SETTINGA
    ELSE
      READ I			; Get displacement
      ADD I TO Y			; Move policeman
      IF COLLISION 0		; If hi player
        OTHER
          IF H = 0
            LET FRAME 3		; If so, change player in fire
            SPAWN 2 3		; Plot player dead
            LET H 1
            LET C 0
          ENDIF
        ENDSPRITE
      ENDIF
    ENDIF
  ENDIF

  ANIMATE SLOW

  DATA $ff,0,$fe,0,$fe,0,$fc,0,$fc,0,$fe,0,$ff,0,$ff,0,0,0,0,0,1,0,1,0,2,0,4,0,4,0,2,0,2,0,1,0,0,0

;---------------------------------------------------------------
; Helicopter
;---------------------------------------------------------------

EVENT SPRITETYPE6

  IF X = 0			; If leaving screen left, platform up
    IF Y > 24
      SUBTRACT 40 FROM Y
    ELSE
      LET Y 144
    ENDIF
    LET X 240
  ENDIF

  IF COLLISION 0		; If collision player
    OTHER
      IF H = 0
        LET FRAME 3		; Change player in fire
        SPAWN 2 3		; Plot dead player
        LET H 1
        LET C 0
      ENDIF
    ENDSPRITE
  ENDIF

  ANIMATE			; Animate rotor

  IF Y <> 64			; If not on platform2

    IF U 0			; Check if bonusbar falling
      READ V			; Get dropping point
       LET U 1			; No, set Bonusbar flag
    ENDIF

    IF W < 8			; If less than 8 bonusbars
      IF X = V			; If helicopter at dropping point
        LET I X
        LET J Y
        DIVIDE I BY 8
        MULTIPLY I BY 8
        ADD 31 TO J
        GETBLOCK I,J
        IF BLOCK EMPTYBLOCK	; Check for static enemy
          ADD 8 TO J
          GETBLOCK I,J
          IF BLOCK PLATFORMBLOCK; Check for platform
            SPAWN 7 9		; If ok, drop bonusbar
            SPAWNED
              LET X I
              LET SETTINGA 0
            ENDSPRITE
            ADD 1 TO W		; Increment bonusbar counter
          ENDIF
        ENDIF
      ENDIF
    ENDIF

  ENDIF

  IF C 0			; Stop if balloon
    SPRITELEFT			; Move helicopter
    SPRITELEFT
  ENDIF

  DATA 200,56,188,24,76,40,108	; Bonusbar dropping points

;---------------------------------------------------------------
; Falling bonusbar
;---------------------------------------------------------------

EVENT SPRITETYPE7

  IF SETTINGA 8			; If bar at platform
    TABLEFALL
    IF AIRBORNE = 0		; If on platform
      REMOVE			; Remove bar
      LET LINE = Y
      LET COLUMN = X
      DIVIDE LINE BY 8
      DIVIDE COLUMN BY 8
      ADD 1 TO LINE
      LET O LINE
      LET P COLUMN
      LET SETTINGA 0
      LET U 0			; Reset bonusbar falling flag
    ENDIF
    SETTINGA 0
  ELSE
    ADD 1 TO SETTINGA		; If not on platform, fall
  ENDIF

;---------------------------------------------------------------
; Balloon
;---------------------------------------------------------------

EVENT SPRITETYPE8

  IF C 0			; Player landed on platform
    REMOVE			; Erase balloon
  ENDIF

  ADD 1 TO Y			; Move down

;---------------------------------------------------------------
; Initialise sprite
;---------------------------------------------------------------

EVENT INITSPRITE

  IF TYPE 0			; If player

    LET I SCREEN		; Select theme
    LET J SCREEN
    DIVIDE J BY 3
    MULTIPLY J BY 3
    SUBTRACT J FROM I

    IF I 0
      LET FRAME 0		; Player on motorbike
    ENDIF

    IF I 1
      LET FRAME 1		; Player on jetski
    ENDIF

    IF I 2
      LET FRAME 2		; Player on skies
    ENDIF
  ENDIF

  IF TYPE 2			; If horizontal moving enemy
     LET DIRECTION 3
     LET SETTINGA 0
     TABLEJUMP
  ENDIF

  IF TYPE 3			; If Spider
     LET SETTINGA 0
     LET FRAME 0
  ENDIF

  IF TYPE 4			; If spider string
     LET SETTINGA 0
     LET FRAME 0
  ENDIF

  IF TYPE 5			; Set delaytime policeman
    GETRANDOM 12
    LET SETTINGA RND
  ENDIF

